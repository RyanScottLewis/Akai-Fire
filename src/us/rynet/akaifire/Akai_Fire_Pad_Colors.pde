void setup() {
  size(400, 100);
  background(0);
  // frameRate(30);

  midiBus          = new MidiBus();
  akaiFire         = new AkaiFire();
  applicationState = new ApplicationState();
  paintState       = new PaintState();

  padVisualizer = new PadVisualizer(akaiFire);

  midiBus.addInput(AkaiFire.DEVICE_NAME);
  midiBus.addOutput(AkaiFire.DEVICE_NAME);

  midiBus.addMidiListener(new ReportingMidiListener(akaiFire));
  midiBus.addMidiListener(new ControlChangeMidiListener(akaiFire));

  for (Pad pad : akaiFire.pads)
    pad.addListener(new PadChangeListener(akaiFire, midiBus, paintState));

  akaiFire.controlButtons.get(0).addListener(new ClearListener(akaiFire, midiBus));
  akaiFire.controlButtons.get(1).addListener(new InvertListener(akaiFire, midiBus));
  akaiFire.controlButtons.get(2).addListener(new PadColorSingularColorizeListener(akaiFire, midiBus, paintState));
  akaiFire.controlButtons.get(4).addListener(new KnobLargeStepListener(akaiFire));

  akaiFire.knobs.get(0).addListener(new ColorKnobListener(akaiFire, paintState, 0));
  akaiFire.knobs.get(1).addListener(new ColorKnobListener(akaiFire, paintState, 1));
  akaiFire.knobs.get(2).addListener(new ColorKnobListener(akaiFire, paintState, 2));

  for (int x = 0; x < 128; x++) {
    for (int y = 0; y < 64; y++) {

      if (x % 4 == 0 && y % 4 == 0)
        akaiFire.screen.plotPixel(x, y, 1);

    }
  }

  akaiFire.screen.setBitmap(bitmap);
  akaiFire.screen.midiSend(midiBus);

  akaiFire.pads.midiSend(midiBus);

  paintMode = paintState.mode;
}

void draw() {

  if (applicationState.mode == ApplicationState.Mode.PAINT) {

    if (paintState.mode != paintMode) { // Paint mode was changed
      paintMode = paintState.mode;

      if (paintState.mode == PaintState.Mode.DRAW) {
        akaiFire.pads.midiSend(midiBus);
      } else if (paintState.mode == PaintState.Mode.PREVIEW) {
        akaiFireBuffer.pads.midiSend(midiBus);
      }
    }

    if (paintState.mode == PaintState.Mode.DRAW) {
    } else if (paintState.mode == PaintState.Mode.PREVIEW) {

      for (Pad pad : akaiFireBuffer.pads)
        pad.setColor( paintState.getTint() );

      akaiFireBuffer.pads.midiSend(midiBus);

      if (millis() - paintState.modeChangedAt >= 500)
        paintState.mode = PaintState.Mode.DRAW;

    }

  }

  background(0);
  padVisualizer.draw();

}
